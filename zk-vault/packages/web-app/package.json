{
  "name": "@zk-vault/web-app",
  "version": "0.1.0",
  "description": "ZK-Vault web application",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf dist coverage",
    "dev:debug": "vite --mode development --debug",
    "dev:https": "vite --mode development --https",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:profile": "vite build --mode production --profile",
    "test:debug": "npm run test -- --no-coverage --verbose",
    "test:ui": "vitest --ui",
    "quality:complexity": "npx ts-complex --threshold 10 src/",
    "quality:deps": "npx depcheck && npm audit",
    "quality:bundle": "npm run build:analyze",
    "quality:all": "npm run lint && npm run type-check && npm run test && npm run quality:complexity",
    "perf:lighthouse": "lhci autorun",
    "perf:bundle": "npm run build:analyze",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:audit": "npm audit fix",
    "clean:full": "npm run clean && rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "firebase": "^10.7.0",
    "@firebase/auth": "^1.5.0",
    "@firebase/firestore": "^4.4.0",
    "@firebase/functions": "^0.10.0",
    "@firebase/storage": "^0.12.0",
    "tailwindcss": "^4.0.0",
    "@zk-vault/shared": "^0.1.0",
    "@zk-vault/crypto": "^0.1.0-mock",
    "fuse.js": "^7.0.0",
    "comlink": "^4.4.1",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "@tailwindcss/vite": "^4.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.22"
  },
  "author": "ZK-Vault Team",
  "license": "MIT"
}