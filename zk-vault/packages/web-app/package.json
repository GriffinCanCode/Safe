{
  "name": "@zk-vault/web-app",
  "version": "0.1.0",
  "description": "ZK-Vault web application",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:formatted": "NODE_ENV=development vite build",
    "build:production": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:css": "stylelint \"src/**/*.{css,vue}\" --fix",
    "test": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,vue,css}\"",
    "format:tailwind": "prettier --write \"src/**/*.{vue,html}\" --plugin=prettier-plugin-tailwindcss",
    "clean": "rm -rf dist coverage",
    "dev:debug": "vite --mode development --debug",
    "dev:https": "vite --mode development --https",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:profile": "vite build --mode production --profile",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css",
    "build:css:formatted": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --watch",
    "build:css:minified": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --minify",
    "build:css:watch": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --watch",
    "test:debug": "npm run test -- --no-coverage --verbose",
    "test:ui": "vitest --ui",
    "quality:complexity": "npx ts-complex --threshold 10 src/",
    "quality:deps": "npx depcheck && npm audit",
    "quality:bundle": "npm run build:analyze",
    "quality:all": "npm run lint && npm run lint:css && npm run type-check && npm run test && npm run quality:complexity",
    "perf:lighthouse": "lhci autorun",
    "perf:bundle": "npm run build:analyze",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:audit": "npm audit fix",
    "clean:full": "npm run clean && rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@firebase/app": "^0.9.25",
    "@firebase/auth": "^1.5.0",
    "@firebase/firestore": "^4.4.0",
    "@firebase/functions": "^0.10.0",
    "@firebase/storage": "^0.12.0",
    "@formkit/auto-animate": "^0.8.2",
    "@headlessui/vue": "^1.7.23",
    "@stablelib/xchacha20poly1305": "^2.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@vueuse/core": "^13.3.0",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/motion": "^3.0.3",
    "@zk-vault/crypto": "file:../crypto",
    "@zk-vault/shared": "file:../shared",
    "comlink": "^4.4.1",
    "firebase": "^10.7.0",
    "fuse.js": "^7.0.0",
    "lottie-web": "^5.13.0",
    "pako": "^2.1.0",
    "pinia": "^2.1.7",
    "tailwindcss": "^4.1.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.3.8",
    "vue-lottie-player": "^0.1.9",
    "vue-motion-one": "^0.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.2",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-prettify": "^0.3.4",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.27"
  },
  "author": "ZK-Vault Team",
  "license": "MIT"
}
